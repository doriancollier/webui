{
  "name": "@dorkos/shared",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./schemas": {
      "types": "./src/schemas.ts",
      "default": "./dist/schemas.js"
    },
    "./transport": {
      "types": "./src/transport.ts",
      "default": "./dist/transport.js"
    },
    "./types": {
      "types": "./src/types.ts",
      "default": "./dist/types.js"
    },
    "./constants": {
      "types": "./src/constants.ts",
      "default": "./dist/constants.js"
    },
    "./config-schema": {
      "types": "./src/config-schema.ts",
      "default": "./dist/config-schema.js"
    },
    "./roadmap-schemas": {
      "types": "./src/roadmap-schemas.ts",
      "default": "./dist/roadmap-schemas.js"
    },
    "./relay-schemas": {
      "types": "./src/relay-schemas.ts",
      "default": "./dist/relay-schemas.js"
    },
    "./mesh-schemas": {
      "types": "./src/mesh-schemas.ts",
      "default": "./dist/mesh-schemas.js"
    },
    "./manifest": {
      "types": "./src/manifest.ts",
      "default": "./dist/manifest.js"
    },
    "./logger": {
      "types": "./src/logger.ts",
      "default": "./dist/logger.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ."
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@dorkos/typescript-config": "workspace:*",
    "@types/node": "^25.3.2",
    "vitest": "^4.0.18"
  }
